octopus:
  scheduler: local:///
  queue: multi
  sandboxRoot: file:///tmp/
  preferences:
    octopus.adaptors.local.queue.multi.maxConcurrentJobs: 4
  poll:
   interval: 30000
   cancelTimeout: 3600000
   deleteTimeout: 43200000
http:
  port: 9998
