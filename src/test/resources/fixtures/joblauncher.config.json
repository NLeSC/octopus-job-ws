{
	"macs": [{
		"id": "id",
		"key": "key",
		"scope": "http://localhost",
		"algorithm": "hmac-sha-1"
	}],
    "xenon": {
        "scheduler": {
            "scheme": "local",
            "queue": "multi"
        },
        "sandbox": {
            "scheme": "file",
            "location": "/",
            "path": "/tmp/sandboxes"
        },
        "preferences": {
            "xenon.adaptors.local.queue.multi.maxConcurrentJobs": 4
        },
        "poll": {
           "interval": 30000,
           "cancelTimeout": 3600000,
           "deleteTimeout": 43200000
        }
    }
}
