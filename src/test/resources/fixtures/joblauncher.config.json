{
	"macs": [{
		"id": "id",
		"key": "key",
		"scope": "http://localhost",
		"algorithm": "hmac-sha-1"
	}],
    "xenon": {
        "launchers": {
            "local": {
                "scheduler": {
                    "scheme": "local",
                    "queue": "multi"
                },
                "sandbox": {
                    "scheme": "file",
                    "location": "/",
                    "path": "/tmp/sandboxes"
                }
            }
        },
        "defaultLauncher": "local",
        "preferences": {
            "xenon.adaptors.local.queue.multi.maxConcurrentJobs": 4
        },
        "poll": {
           "interval": 30000,
           "cancelTimeout": 3600000,
           "deleteTimeout": 43200000
        }
    }
}
